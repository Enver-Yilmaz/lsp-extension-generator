{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "any",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.any",
				"match": "\\b(keywordsGoHere)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.any",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.any",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "any.ext"
}